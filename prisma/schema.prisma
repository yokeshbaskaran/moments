generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
  //import client from this path(Prismaclient)
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id          String   @id @default(uuid())
  title       String
  description String
  tags        String
  image       String
  userId      String
  username    String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
